<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.apache.ibatis.annotations.Mapper">
    <resultMap id="BaseResultMap" type="com.jcx.military.project.data.Etag">
        <id column="id" jdbcType="VARCHAR" property="id" />
        <result column="etag" jdbcType="VARCHAR" property="etag" />

    </resultMap>
    <!--  <cache/>-->
    <sql id="Base_Column_List">
        id, etag
    </sql>
    <select id="selectAll"  resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from tab_user where id = #{id,jdbcType=VARCHAR}
    </select>

</mapper>